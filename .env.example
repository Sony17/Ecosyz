# Example environment variables for Open Idea workspace
DATABASE_URL=
YOUTUBE_API_KEY=

# Supabase configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Optional: For social logins (configure in Supabase dashboard)
# GITHUB_CLIENT_ID=
# GITHUB_CLIENT_SECRET=
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=

# Payment Configuration
NEXT_PUBLIC_UPI_ID=your-business-upi-id@bank
NEXT_PUBLIC_PAYPAL_CLIENT_ID=your-paypal-client-id

# EmailJS Configuration (for contact forms)
NEXT_PUBLIC_EMAILJS_SERVICE_ID=your-emailjs-service-id
NEXT_PUBLIC_EMAILJS_TEMPLATE_ID=your-emailjs-template-id
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=your-emailjs-public-key
NEXT_PUBLIC_COMPANY_EMAIL=your-company-email@example.com

# AI Summarization Service (OpenAI)
OPENAI_API_KEY=your-openai-api-key

# Redis Caching (Upstash)
UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-redis-token

